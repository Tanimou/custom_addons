<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for Campaign Codes -->
        <record id="seq_asset_inventory_campaign" model="ir.sequence">
            <field name="name">Campagne Inventaire Immobilisations</field>
            <field name="code">asset.inventory.campaign</field>
            <field name="prefix">INV/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>
