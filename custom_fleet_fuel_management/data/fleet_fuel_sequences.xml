<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">
    <record id="seq_fleet_fuel_card" model="ir.sequence">
        <field name="name">Séquence cartes carburant</field>
        <field name="code">fleet.fuel.card</field>
        <field name="prefix">FCA-%(year)s-</field>
        <field name="padding">4</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_fleet_fuel_recharge" model="ir.sequence">
        <field name="name">Séquence recharges carburant</field>
        <field name="code">fleet.fuel.recharge</field>
        <field name="prefix">FRC-%(year)s-</field>
        <field name="padding">5</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_fleet_fuel_expense" model="ir.sequence">
        <field name="name">Séquence dépenses carburant</field>
        <field name="code">fleet.fuel.expense</field>
        <field name="prefix">FEX-%(year)s-</field>
        <field name="padding">6</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_fleet_fuel_summary" model="ir.sequence">
        <field name="name">Séquence synthèses carburant</field>
        <field name="code">fleet.fuel.monthly.summary</field>
        <field name="prefix">FSM-%(year)s-</field>
        <field name="padding">3</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>
</data>
</odoo>
