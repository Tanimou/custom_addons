<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Séquence pour les codes véhicules VEH-#### -->
        <record id="sequence_fleet_vehicle_code" model="ir.sequence">
            <field name="name">Code Véhicule</field>
            <field name="code">fleet.vehicle.code</field>
            <field name="prefix">VEH-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les missions MIS-#### -->
        <record id="sequence_fleet_mission" model="ir.sequence">
            <field name="name">Mission Véhicule</field>
            <field name="code">fleet.mission</field>
            <field name="prefix">MIS-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les ordres de mission OMI-#### -->
        <record id="sequence_fleet_mission_order" model="ir.sequence">
            <field name="name">Ordre de Mission</field>
            <field name="code">fleet.mission.order</field>
            <field name="prefix">OMI-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
