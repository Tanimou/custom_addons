<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Prélèvement ticket numbering -->
        <record id="seq_prelevement_ticket" model="ir.sequence">
            <field name="name">Prélèvement Ticket Sequence</field>
            <field name="code">prelevement.ticket</field>
            <field name="prefix">%(month)s-%(y)s-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
        </record>
    </data>
</odoo>
