<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_template_inherit_form_view" model="ir.ui.view">
            <field name="name">budget.analytic.inherit.form</field>
            <field name="model">budget.analytic</field>
            <field name="inherit_id" ref="account_budget.view_budget_analytic_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="account_analytic_id" readonly='1'/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>