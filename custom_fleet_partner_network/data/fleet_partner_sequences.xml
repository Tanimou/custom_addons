<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquences pour les contrats partenaires (Phase 2) -->
        <record id="seq_fleet_partner_contract" model="ir.sequence">
            <field name="name">Fleet Partner Contract Sequence</field>
            <field name="code">fleet.partner.contract</field>
            <field name="prefix">FPC-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Séquence pour les tickets d'incident (Phase 3) -->
        <record id="seq_fleet_incident_ticket" model="ir.sequence">
            <field name="name">Fleet Incident Ticket Sequence</field>
            <field name="code">fleet.incident.ticket</field>
            <field name="prefix">PNR-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>
